<section class="content-header">
	<h1>Admin</h1>
</section>
<section class="content">
	<div class="text-centre"><%= link_to "Add New Admin",admin_admin_new_path,:class => "btn btn-primary"%></div><br/>
	<div class="row">
		<div class="col-xs-12">
			<div class="box">

				<div class="box-body table-responsive">
					<table id="example1" class="table table-bordered table-striped">
						<thead>
							<tr>
								<th>Sr.No.</th>
								<th>Name</th>
								<th>Email</th>
								<!-- <th></th> -->
							</tr>	
						</thead>
						<% @count = 0 %>
						<% @users.each do |user|%>
						<tbody>
							<tr>
								<% @count += 1 %>
								<td><%=  @count %></td>
								<td><%= user.name %></td>
								<td><%= user.email %></td>
								<!-- <td>
									<%#= link_to "<i class='glyphicon glyphicon-eye-open'></i>".html_safe,  admin_admin_show_path(user.id),:class => " btn btn-default btn-xs" %>
									<%#= link_to "<i class='glyphicon glyphicon-pencil'></i>".html_safe, admin_admin_edit_path(user),:class => " btn btn-default btn-xs" %>
									 <%#= link_to "<i class='glyphicon glyphicon-trash'></i>".html_safe, admin_admin_destroy_path(user.id),  data: { confirm: 'Are you sure?' },method: :delete,:class => " btn btn-default btn-xs" %> 
								</td> -->
							</tr>
						</tbody>
						<% end %>
					</table>

					<% if @users.count > 10 %>
					<br/>
					<div class="digg_pagination">
						<%= will_paginate @users, :container => false %>
					</div>
					<% end %>
				</div>					
			</div>						
		</div>							
	</div>								
</section>